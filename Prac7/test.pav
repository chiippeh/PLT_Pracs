void main () {
// char01.pav
// Very simple allowable operations on characters
    //write("a" "b" + "cccc" "y");
    int a = 5;
    int b = 5;
    int c = 0;
    int d = 2;
    if (a == b) c := d;
    write(d);
    if (a = b) then c := d;
    write(d);
    if (a = b) then c := d;
    write(d);
    if (a = b) c = d;
    write(d);
    if (a <> b) c = d;
    write(d);

    // legal version
    if (a == b) c = d;
    write(d);
}
