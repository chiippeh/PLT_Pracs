void main () {
// Print a table of factorial numbers 1! ... 10!
// Wynne Edwards (g21E2079), Mila-Jo Davies (g21D6937), Manu Jourdan (g21J5408), Rhodes University, 2023


  const limit = 10;
  int n = 1;
  while (n <= limit) {
    int f = 1;
    int i = n;
    while (i > 0) {
      f = f * i;
      i = i - 1;
    }
    write(n, "! = ", f, "\n");
    n = n + 1;
  }
} // main
